{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nest Framework",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start:debug",
      ],
      "autoAttachChildProcesses": true,
      "restart": false,
      "sourceMaps": true,
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
    }
  ]
}