generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters", "multiSchema", "views", "prismaSchemaFolder", "relationJoins"]
  binaryTargets   = ["native", "debian-openssl-1.1.x", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("dbConnectionString")
  schemas  = ["public", "permission", "mdb", "dsp", "dpom","job"]
}
