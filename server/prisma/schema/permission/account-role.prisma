model AccountRole {
  emplid  String  @map("emplid") @db.VarChar(11)
  account Account @relation(fields: [emplid], references: [emplid], onDelete: Cascade, onUpdate: NoAction)

  roleId String @map("role_id") @db.Uuid
  role   Role   @relation(fields: [roleId], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([emplid, roleId])
  @@map("account_role")
  @@schema("permission")
}
