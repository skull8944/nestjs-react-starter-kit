{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22"
  },
  "prisma": {
    "schema": "prisma/schema"
  },
  "scripts": {
    "build": "nest build",
    "check-update": "npx npm-check-updates",
    "eslint-output": "eslint-output",
    "format": "pretty-quick --staged --ignore-unknown --pattern '**/*.(ts|js|md|json)'",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config=jest.config.json --maxWorkers=4 --forceExit --coverage",
    "test:watch": "jest -config=jest.config.json --watch",
    "test:cov": "jest -config=jest.config.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prisma:migrate": "npx prisma migrate dev --create-only",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:pull": "npx prisma db pull",
    "prisma:generate": "npx prisma generate",
    "seed": "ts-node ./src/seeds/permission.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@azure/msal-node": "^3.7.3",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "6.15.0",
    "async-mutex": "^0.5.0",
    "bullmq": "^5.58.5",
    "cache-manager": "^7.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "cron-parser": "^5.3.1",
    "dayjs": "^1.11.18",
    "handlebars": "^4.7.8",
    "nodemailer": "^7.0.6",
    "prisma": "6.15.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/plugin-kit": "^0.3.5",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.1",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-output": "^4.0.2",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
